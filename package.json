{
  "name": "environment-normalize",
  "version": "1.0.1",
  "description": "A lightweight utility for normalizing environment names",
  "main": "dist/index.js",
  "author": "Nate Clark <n8@n8io.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean:dusting && npm run build:babel",
    "clean": "npm run clean:dusting && npm run clean:modules",
    "prepublish": "npm run lint",
    "pretest": "npm run lint",
    "build:babel": "babel -d ./dist ./src --source-maps --ignore node_modules,build,coverage,.nyc_output,./**/*.spec.js",
    "clean:modules": "rm -rf node_modules/",
    "clean:dusting": "rm -rf .nyc_output/ coverage/ dist/ npm-debug.log",
    "lint": "eslint src/**/*.js ./*.js --quiet --fix",
    "start": "echo \"This is not a runnable library.\" && exit 1",
    "test": "nyc ava",
    "test:watch": "ava -wv"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8io/environment-normalize.git"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "src/**/*.spec.js",
      "!.nyc_output/",
      "!coverage/",
      "!dist/"
    ],
    "require": "babel-register"
  },
  "nyc": {
    "check-coverage": true,
    "cache": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "coverage",
      "node_modules",
      "dist"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ]
  },
  "devDependencies": {
    "ava": "^0",
    "babel-cli": "^6",
    "babel-preset-es2015": "^6",
    "babel-register": "^6",
    "eslint": "^3",
    "eslint-config-n8io": "^1",
    "nyc": "^8",
    "source-map-support": "^0"
  },
  "runkitExampleFilename": "example.js"
}
